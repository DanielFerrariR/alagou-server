{
  "name": "track-server",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node --files --log-error src",
    "dev": "nodemon",
    "commit": "git add . && git-cz",
    "lint": "eslint --ext .js,.ts,.md .",
    "prettier": "prettier \"**/*.+(js|json|ts|md|eslintrc|prettierrc)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "tsc": "tsc",
    "check-types": "yarn tsc",
    "validate": "npm-run-all --parallel check-types check-format lint",
    "tunnel": "ngrok http 3005"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/express": "4.17.7",
    "@types/mongoose": "5.7.36",
    "@types/multer": "1.4.4",
    "@types/nodemailer": "6.4.0",
    "@types/papaparse": "5.2.2",
    "@types/jsonwebtoken": "8.5.0",
    "bcrypt": "5.0.0",
    "body-parser": "1.19.0",
    "cloudinary": "1.23.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.9.28",
    "multer": "1.4.2",
    "multer-storage-cloudinary": "4.0.0",
    "ngrok": "3.2.7",
    "njwt": "1.0.0",
    "nodemailer": "6.4.11",
    "nodemon": "2.0.4",
    "papaparse": "5.3.0",
    "socket.io": "2.3.0",
    "ts-node": "8.10.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "3.7.0",
    "@typescript-eslint/parser": "3.7.0",
    "babel-eslint": "10.1.0",
    "commitizen": "4.1.2",
    "eslint": "7.5.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-airbnb-typescript": "9.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-mdx": "1.7.1",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "remark-preset-lint-consistent": "3.0.1",
    "remark-preset-lint-markdown-style-guide": "3.0.1",
    "remark-preset-lint-recommended": "4.0.1",
    "remark-preset-prettier": "0.4.0",
    "typescript": "3.9.7"
  }
}
